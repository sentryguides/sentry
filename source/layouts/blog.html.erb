<% wrap_layout :layout do %>
  <div class="row">
    <div class="col-md-9">
      <div class="card">
        <div class="card-block">
          <time><%= current_page.date.strftime('%b %e %Y') %></time>
          <h1><%= current_page.title %></h1>
          <%= yield %>
        </div>
      </div>
    </div>
    <div class="col-md-3">
    </div>
  </div>
<% end %>
